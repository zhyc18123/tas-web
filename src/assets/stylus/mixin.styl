
// 不换行
no-wrap()
  text-overflow: ellipsis
  overflow: hidden
  white-space: nowrap

// 列表表单样式
table-form()
  background: #f7f7f7
  border: 1px solid #f1f1f1
  padding: 20px 20px 0
  .el-form-item
    margin-bottom: 20px
    .new-btn
      border-radius: 30px
      background: #1ebd99
      border-color: #1ebd99
    &.new-item
      float: right
      margin-right: 0